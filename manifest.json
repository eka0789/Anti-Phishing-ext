{
  "manifest_version": 3,
  "name": "Phishing Detector MVP",
  "version": "1.0",
  "description": "Deteksi URL phishing secara real-time dan blokir akses berbahaya.",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
